<Database ServerName="loop.qa.ptssql.com-do not run" Port="0" DatabaseName="Loop" CredentialsName="" Locale="en">
	<Tables>
		<Table Name="[dbo].[MVR]" DisableTriggers="false" DisableConstraints="false">
			<Columns>
				<Column Name="[LicenseNumber]">
					<Transforms>
						<Transform Transform="Faker.Random.ReplaceNumbers(&quot;########&quot;)" TransformType="expression" WhereClause="" />
					</Transforms>
				</Column>
				<Column Name="[LicenseState]">
					<Transforms>
						<Transform Transform="Faker.Address.StateAbbr()" TransformType="expression" WhereClause="" />
					</Transforms>
				</Column>
				<Column Name="[LastName]">
					<Transforms>
						<Transform Transform="Faker.Name.LastName().ToUpper()" TransformType="expression" WhereClause="" />
					</Transforms>
				</Column>
				<Column Name="[FirstName]">
					<Transforms>
						<Transform Transform="Faker.Name.FirstName().ToUpper()" TransformType="expression" WhereClause="" />
					</Transforms>
				</Column>
				<Column Name="[MiddleName]">
					<Transforms>
						<Transform Transform="Faker.Name.LastName().Substring(0, 1)" TransformType="expression" WhereClause="[MiddleName] IS NOT NULL AND LEN([MiddleName]) = 1" />
						<Transform Transform="Faker.Name.LastName()" TransformType="expression" WhereClause="[MiddleName] IS NOT NULL AND LEN([MiddleName]) &gt; 1" />
					</Transforms>
				</Column>
				<Column Name="[FullName]">
					<Transforms>
						<Transform Transform="String.Concat(RowValues.FirstName, &quot; &quot;, RowValues.MiddleName, &quot; &quot;, RowValues.LastName)" TransformType="expression" WhereClause="[MiddleName] IS NOT NULL" />
						<Transform Transform="String.Concat(RowValues.FirstName, &quot; &quot;, RowValues.LastName)" TransformType="expression" WhereClause="[MiddleName] IS NULL" />
					</Transforms>
				</Column>
				<Column Name="[Alias]">
					<Transforms>
						<Transform Transform="TestFiles\TestPS.ps1" TransformType="powershell" WhereClause="" />
					</Transforms>
				</Column>
				<Column Name="[DateOfBirth]">
					<Transforms>
						<Transform Transform="Faker.Date.Between(DateTime.Now.AddYears(-70), DateTime.Now.AddYears(-18))" TransformType="expression" WhereClause="" />
					</Transforms>
				</Column>
				<Column Name="[SSN]">
					<Transforms>
						<Transform Transform="Faker.Person.Ssn()" TransformType="expression" WhereClause="" />
					</Transforms>
				</Column>
				<Column Name="[Address]">
					<Transforms>
						<Transform Transform="String.Concat(Faker.Address.BuildingNumber(), &quot; &quot;, Faker.Address.StreetName())" TransformType="expression" WhereClause="" />
					</Transforms>
				</Column>
				<Column Name="[City]">
					<Transforms>
						<Transform Transform="Faker.Address.City()" TransformType="expression" WhereClause="" />
					</Transforms>
				</Column>
				<Column Name="[State]">
					<Transforms>
						<Transform Transform="Faker.Address.State()" TransformType="expression" WhereClause="" />
					</Transforms>
				</Column>
				<Column Name="[Zip]">
					<Transforms>
						<Transform Transform="Faker.Address.ZipCode()" TransformType="expression" WhereClause="" />
					</Transforms>
				</Column>
			</Columns>
			<SqlScripts />
		</Table>
		<Table Name="[dbo].[Insured]" DisableTriggers="false" DisableConstraints="false">
			<Columns>
				<Column Name="[fname1]">
					<Transforms>
						<Transform Transform="Faker.Name.FirstName()" TransformType="expression" WhereClause="" />
					</Transforms>
				</Column>
				<Column Name="[lname1]">
					<Transforms>
						<Transform Transform="Faker.Name.LastName()" TransformType="expression" WhereClause="" />
					</Transforms>
				</Column>
				<Column Name="[insuredName]">
					<Transforms>
						<Transform Transform="String.Concat(RowValues.fname1, &quot; &quot;, RowValues.lname1)" TransformType="expression" WhereClause="" />
					</Transforms>
				</Column>
				<Column Name="[phone2]">
					<Transforms>
						<Transform Transform="Faker.Phone.PhoneNumber()" TransformType="expression" WhereClause="" />
					</Transforms>
				</Column>
				<Column Name="[email]">
					<Transforms>
						<Transform Transform="Faker.Internet.Email()" TransformType="expression" WhereClause="" />
					</Transforms>
				</Column>
			</Columns>
			<SqlScripts />
		</Table>
	</Tables>
