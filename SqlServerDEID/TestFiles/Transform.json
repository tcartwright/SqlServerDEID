            {
              "Transform": "Faker.Name.LastName()",
              "TransformType": "expression",
              "WhereClause": "[MiddleName] IS NOT NULL AND LEN([MiddleName]) > 1"
            }
          ]
        },
        {
          "Name": "[FullName]",
          "Transforms": [
            {
              "Transform": "String.Concat(RowValues.FirstName, \" \", RowValues.MiddleName, \" \", RowValues.LastName)",
